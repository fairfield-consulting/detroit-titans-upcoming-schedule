{
  "name": "next-titans",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "prisma migrate deploy && prisma generate && remix build",
    "dev": "concurrently --kill-others \"npm:*:dev\"",
    "remix:dev": "remix dev --manual",
    "inngest:dev": "npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest",
    "start": "remix-serve ./build/index.js",
    "lint": "eslint app/**/*.{ts,tsx}",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.4.0",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@types/node": "20.6.4",
    "autoprefixer": "10.4.16",
    "hast-util-from-html": "^2.0.1",
    "hast-util-select": "^6.0.1",
    "inngest": "^3.0.1",
    "isbot": "^3.6.8",
    "luxon": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/hast": "^3.0.1",
    "@types/luxon": "^3.3.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "bun-types": "^1.0.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.4.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
