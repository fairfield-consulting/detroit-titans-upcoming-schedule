{
  "name": "titans-sports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm:*:dev\"",
    "next:dev": "next dev",
    "inngest:dev": "npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest",
    "build": "bun db:migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "drizzle-kit check:sqlite || bun run src/db/migrate.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.3.5",
    "better-sqlite3": "^8.7.0",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "hast-util-from-html": "^2.0.1",
    "hast-util-select": "^6.0.1",
    "inngest": "^3.0.2",
    "luxon": "^3.4.3",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/hast": "^3.0.1",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "bun-types": "^1.0.5-canary.20231008T140131",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "encoding": "^0.1.13",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
